{
  "name": "inncol-date-range-picker",
  "version": "1.0.3",
  "description": "Free open source date range picker for MUI using the free version of MUI/ MUI core",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test:build": "npx webpack --mode=production",
    "test:start": "npx webpack serve --mode=development",
    "preversion": "npm run build",
    "version": "npm publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "date-range",
    "date",
    "range",
    "picker",
    "mui",
    "material-ui",
    "free",
    "open-source"
  ],
  "author": "Inncol <contacto@inncol.com.mx>",
  "license": "GPL",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.51",
    "babel-loader": "^9.1.3",
    "html-webpack-plugin": "^5.6.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "date-fns": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5"
  },
  "bugs": {
    "url": "https://github.com/INNCOL/date-time-picker/issues"
  },
  "homepage": "https://github.com/INNCOL/date-time-picker"
}